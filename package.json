{
  "name": "parsepal-uploader",
  "version": "1.0.0",
  "description": "ParsePal Desktop Uploader â€” Auto-upload WoW combat logs",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "tsc -p tsconfig.main.json && concurrently -k \"tsc -p tsconfig.main.json --watch --preserveWatchOutput\" \"vite --port 5173\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "tsc -p tsconfig.main.json && vite build",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac"
  },
  "dependencies": {
    "chokidar": "^3.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
